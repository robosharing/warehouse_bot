      <origin xyz="1.195 0.0 -1.7" rpy="0 0 3.14"/>
      <geometry>
        <mesh filename="file://$(find warehouse_bot)/meshes/mid-360-asm_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link> -->

  <link name="laser_frame_under">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10"/>
      <inertia ixx="0.00068" iyy="0.00068" izz="0.000389" ixy="0.0" iyz="-0.0" ixz="-0.0"/>
    </inertial>
    <visual>
      <origin xyz="-1.475 0 0.005" rpy="3.14 0 0"/>
      <geometry>
        <mesh filename="file://$(find warehouse_bot)/meshes/1line_lidar_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-1.475 0 0.005" rpy="3.14 0 0"/>
      <geometry>
        <mesh filename="file://$(find warehouse_bot)/meshes/1line_lidar_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link> 

  <link name="fork_1">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="10"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.155" />
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 -0.03" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find warehouse_bot)/meshes/fork_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.0 0 -0.03" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find warehouse_bot)/meshes/fork_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <joint name="fork_joint" type="fixed">
    <origin xyz="0.0 0.0 0.022893" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="fork_1"/>
  </joint>


  <xacro:sensor_d435_1 name="camera1" topics_ns="camera1" parent="base_link" publish_pointcloud="true">
    <origin xyz="-0.46 0.19 0.05" rpy="0 0 3.14" />
  </xacro:sensor_d435_1>  

  <xacro:sensor_d435_2 name="camera2" topics_ns="camera2" parent="base_link" publish_pointcloud="true">
    <origin xyz="-0.46 -0.19 0.05" rpy="0 0 3.14" />
  </xacro:sensor_d435_2>  

  <xacro:sensor_d435_3 name="camera3" topics_ns="camera3" parent="base_link" publish_pointcloud="true">
    <origin xyz="1.0 0.0 1.77" rpy="0 2.2 3.14" />
  </xacro:sensor_d435_3>  

  <joint name="16line_lidar_joint" type="fixed">
    <origin xyz="1.1 0.0 2.24" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="line_lidar_1"/>
  </joint>

  <joint name="laser_joint1" type="fixed">
    <origin xyz="1.6 0.0 0.02" rpy="0 0 0"/>
    <parent link="chassis"/>
    <child link="laser_frame_under"/>
  </joint> 

</robot>